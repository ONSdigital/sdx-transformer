{
  "title": "Build Spec for ABS short forms",
  "survey_id": "202",
  "target": "COMMON SOFTWARE",
  "period_format": "YYYYMM",
  "pck_period_format": "YY",
  "template": {
    "9": "$YES_NO_RADIO",
    "11": "$START_DATE",
    "12": "$END_DATE",
    "15": "$YES_NO_RADIO",
    "16": "$YES_NO_RADIO",
    "80": "$YES_NO_RADIO",
    "81": "$PERCENTAGE_RADIO",
    "146": "$COMMENT",
    "163": "$CURRENCY_THOUSANDS",
    "164": "$CURRENCY_THOUSANDS",
    "300": "$CURRENCY_THOUSANDS",
    "311": "$CURRENCY_THOUSANDS",
    "318": "$CURRENCY_THOUSANDS",
    "327": "$CURRENCY_THOUSANDS",
    "337": "$CURRENCY_THOUSANDS",
    "339": "$CURRENCY_THOUSANDS",
    "346": "$CURRENCY_THOUSANDS",
    "399": "$CURRENCY_THOUSANDS",
    "400": "$CURRENCY_THOUSANDS",
    "403": "$CURRENCY_THOUSANDS",
    "412": "$CURRENCY_THOUSANDS",
    "414": "$CURRENCY_THOUSANDS",
    "415": "$CURRENCY_THOUSANDS",
    "416": "$CURRENCY_THOUSANDS",
    "419": "$CURRENCY_THOUSANDS",
    "420": "$CURRENCY_THOUSANDS",
    "431": "$CURRENCY_THOUSANDS",
    "450": "$CURRENCY_THOUSANDS",
    "455": "$CURRENCY_THOUSANDS",
    "475": "$CURRENCY_THOUSANDS",
    "476": "$CURRENCY_THOUSANDS",
    "499": "$499",
    "500": "$CURRENCY_THOUSANDS",
    "501": "$CURRENCY_THOUSANDS",
    "502": "$CURRENCY_THOUSANDS",
    "599": "$CURRENCY_THOUSANDS",
    "600": "$CURRENCY_THOUSANDS",
    "699": "$CURRENCY_THOUSANDS"
  },
  "transforms": {
    "$START_OF_PERIOD": {
      "name": "START_OF_YEAR",
      "args": {
        "value": "#period_id",
        "input_format": "YYYYMM",
        "display_as": "DDMMYY"
      }
    },
    "$END_OF_PERIOD": {
      "name": "END_OF_YEAR",
      "args": {
        "value": "#period_id",
        "input_format": "YYYYMM",
        "display_as": "DDMMYY"
      }
    },
    "$TO_DATE": {
      "name": "TO_DATE",
      "args": {
        "display_as": "DDMMYY"
      }
    },
    "$START_DATE": {
      "name": "EXISTS",
      "args": {
        "on_true": "$TO_DATE",
        "on_false": "$START_OF_PERIOD"
      }
    },
    "$END_DATE": {
      "name": "EXISTS",
      "args": {
        "on_true": "$TO_DATE",
        "on_false": "$END_OF_PERIOD"
      }
    },
    "$PERCENTAGE_RADIO": {
      "name": "LOOKUP",
      "args": {
        "0-9%": "10000",
        "10-24%": "1000",
        "25-49%": "100",
        "50-74%": "10",
        "75-100%": "1",
        "on_no_match": "0"
      }
    },
    "$YES_NO_RADIO": {
      "name": "LOOKUP",
      "args": {
        "Yes": "10",
        "No": "1",
        "on_no_match": "0"
      }
    },
    "$CURRENCY_THOUSANDS": {
      "name": "CURRENCY_THOUSANDS",
      "args": {}
    },
    "$NO_COMMENT_1808": {
      "name": "ANY_CONTAINS",
      "args": {
        "values": ["#form_type"],
        "match_str": "1808",
        "on_true": null,
        "on_false": "&value"
      }
    },
    "$NO_COMMENT_1820": {
      "name": "ANY_CONTAINS",
      "args": {
        "values": ["#form_type"],
        "match_str": "1820",
        "on_true": null,
        "on_false": "&value"
      },
      "post": "$NO_COMMENT_1808"
    },
    "$COMMENT": {
      "name": "EXISTS",
      "args": {
        "on_true": "1",
        "on_false": "2"
      },
      "post": "$NO_COMMENT_1820"
    },
    "$499": {
      "name": "EXISTS",
      "args": {
        "on_true": "$CURRENCY_THOUSANDS",
        "on_false": "$TOTAL"
      }
    },
    "$TOTAL": {
      "name": "TOTAL",
      "args": {
        "value": "0",
        "values": [
          "&403",
          "&420"
        ]
      }
    }
  }
}